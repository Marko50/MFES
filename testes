print user_manager.login("dank","memes")

print user_manager.buyTicket("cenas",ticket_manager,event_manager)
print user_manager.buyTicket("cenas",ticket_manager,event_manager)
print user_manager.buyTicket("cenas",ticket_manager,event_manager)

